shader ComputeColorTemperature<float temperature> : ComputeColor, Texturing
{
    override float4 Compute()
    {
        
        if(temperature>0)
        {
            float temp = temperature/100.0f;
            return float4(temp,0.0,0.0,temp);
        }
        else 
        {
            float temp = -temperature/100.0f;
            return float4(0.0,0.0,temp,temp);
        }
        
            
    }
};